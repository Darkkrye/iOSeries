  pod 'SearchTextField', :git => 'https://github.com/apasccon/SearchTextField'
  pod 'Cosmos', :git => 'https://github.com/marketplacer/Cosmos'
  pod 'ALAccordion', :git => 'https://github.com/Alliants/ALAccordion'

end
